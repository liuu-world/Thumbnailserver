{
    "name": "gregbrown/thumbnailserver",
    "description": "Greg's first package",
    "keywords": [
        "gregbrown",
        "thumbnailserver"
    ],
    "homepage": "https://github.com/gregbrown/thumbnailserver",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Greg Brown",
            "email": "greg@liuu.world",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "GregBrown\\ThumbnailServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GregBrown\\ThumbnailServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GregBrown\\ThumbnailServer\\ThumbnailServerServiceProvider"
            ],
            "aliases": {
                "ThumbnailServer": "GregBrown\\ThumbnailServer\\ThumbnailServerFacade"
            }
        }
    }
}
